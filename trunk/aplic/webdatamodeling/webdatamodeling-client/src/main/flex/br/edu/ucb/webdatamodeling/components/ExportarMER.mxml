<?xml version="1.0" encoding="utf-8"?>
<mx:Panel xmlns:mx="http://www.adobe.com/2006/mxml" title="{resourceManager.getString('messages', 'exportar.tltExportar')}" layout="absolute" width="327" height="163">
	<mx:Script>
		<![CDATA[
			import mx.controls.Alert;
			private function teste():void {
				Alert.show("meu ovo");
			}
		]]>
	</mx:Script>
	<mx:Form width="100%" height="100%">
		<mx:FormItem label="{resourceManager.getString('messages', 'exportar.arquivo')}">
			<mx:TextInput id="nomeArquivo"/>
		</mx:FormItem>
		<mx:FormItem label="{resourceManager.getString('messages', 'exportar.tipoArquivo')}">
			<mx:ComboBox></mx:ComboBox>
		</mx:FormItem>
		<mx:HBox horizontalAlign="center" width="100%">
			<mx:Button label="{resourceManager.getString('messages', 'exportar.btnExportar')}" id="btnExportar" click="teste()"/>
		</mx:HBox>
	</mx:Form>
</mx:Panel>
